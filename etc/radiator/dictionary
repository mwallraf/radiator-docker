#
# Alcatel ESAM
# Note non-standard VSA format with 16 bit VS attribute num
#
VENDOR         Alcatel-ESAM    637    format=2,1
VENDORATTR     637     A-ESAM-VRF-Name                 0x0700  string
VENDORATTR     637     A-ESAM-Vlan-Id                  0x0701  integer
VENDORATTR     637     A-ESAM-QOS-Profile-Name         0x0702  string
VENDORATTR     637     A-ESAM-QOS-Params               0x0703  string
VENDORATTR     637     A-ESAM-Termination-Cause        0x0704  integer
VENDORATTR     637     A-ESAM-AAA                      0x0709  integer
VENDORATTR     637     A-ESAM-ATM                      0x070a  integer
VENDORATTR     637     A-ESAM-Alarm                    0x070b  integer
VENDORATTR     637     A-ESAM-DHCP                     0x070c  integer
VENDORATTR     637     A-ESAM-EQP                      0x070d  integer
VENDORATTR     637     A-ESAM-IGMP                     0x070e  integer
VENDORATTR     637     A-ESAM-CPEproxy                 0x070f  integer
VENDORATTR     637     A-ESAM-IP                       0x0710  integer
VENDORATTR     637     A-ESAM-PPPoE                    0x0711  integer
VENDORATTR     637     A-ESAM-QoS                      0x0712  integer
VENDORATTR     637     A-ESAM-SWMgt                    0x0713  integer
VENDORATTR     637     A-ESAM-Transport                0x0714  integer
VENDORATTR     637     A-ESAM-VLAN                     0x0715  integer
VENDORATTR     637     A-ESAM-XDSL                     0x0716  integer
VENDORATTR     637     A-ESAM-Security                 0x0717  integer
VENDORATTR     637     A-ESAM-Cluster                  0x0718  integer
VENDORATTR     637     A-ESAM-Prompt                   0x0719  string
VENDORATTR     637     A-ESAM-Password-Timeout         0x071a  integer
VENDORATTR     637     A-ESAM-Description              0x071b  string

